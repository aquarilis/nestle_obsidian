        <name>(DXC) Validate duplicated serial</name>
        <order/>
        <script><![CDATA[function onChange(control, oldValue, newValue, isLoading) {
    if (isLoading || newValue == '') {
        return;
    }

	g_form.hideFieldMsg('serial_number');
	var serialsAssociated = this.my_g_form.getValue('obsidian_serial_numbers_associated');
	if (!serialsAssociated)
		return;
	var serials = JSON.parse(serialsAssociated);
	
	for (var i=0; i < serials.length; i++){
		if (serials[i].serial_number == newValue){
			g_form.showFieldMsg('serial_number', 'This serial number is already included. Please add another serial number.', 'error');
			return;
		}
	}

}]]></script>
